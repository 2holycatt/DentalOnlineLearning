  <script>
    function checkCheckboxes() {
      const checkboxes = document.querySelectorAll('input[type="checkbox"]'); // ดึง checkbox ทั้งหมด
      const submitBtn = document.querySelector('.submit-form')
      const checkedCountElement = document.getElementById('checkedCount'); // Element ที่จะแสดงจำนวน checkbox ที่ถูก checked
      // const selectedValues = [];
      // ตรวจสอบว่ามี checkbox ที่ถูกเลือกหรือไม่

      const isChecked = Array.from(checkboxes).some(checkbox => checkbox.checked);

      if (isChecked) {
        submitBtn.disabled = false; // ถ้ามี checkbox ถูกเลือก ยกเลิก disabled ของปุ่ม
      } else {
        submitBtn.disabled = true; // ถ้าไม่มี checkbox ถูกเลือก ให้ปุ่ม disabled เหมือนเดิม
      }

      let checkedCount = 0; // ตัวแปรสำหรับนับจำนวน

      // ตรวจสอบแต่ละ checkbox ว่าถูก checked หรือไม่
      checkboxes.forEach(checkbox => {
        if (checkbox.checked) {
          checkedCount++; // เพิ่มค่าหาก checkbox ถูก checked
          // selectedValues.push(checkbox.value); // เก็บค่าที่ถูกเลือกในอาร์เรย์

