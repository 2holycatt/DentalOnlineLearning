      //     });

      //     console.log(response);
      //     // if (response.ok) {
      //     //   const result = await response.json();
      //     //   console.log('Success:', result);
      //     //   // ทำการอัพเดต UI หรือการจัดการอื่นๆ ตามต้องการ
      //     // } else {
      //     //   console.error('Error:', response.statusText);
      //     // }
      //   } catch (error) {
      //     console.error('Error:', error);
      //   }
      // };
    }

    // เพิ่ม event listener ให้ checkbox ทุกอัน
    document.querySelectorAll('input[type="checkbox"]').forEach(checkbox => {
      checkbox.addEventListener('change', checkCheckboxes); // เรียกฟังก์ชันเมื่อสถานะ checkbox เปลี่ยน
    });

    // เรียกฟังก์ชันครั้งแรกเพื่อตั้งค่าเริ่มต้น
